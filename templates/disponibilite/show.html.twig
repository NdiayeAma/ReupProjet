{% extends 'base.html.twig' %}

{% block title %}Disponibilite{% endblock %}

{% block body %}
    <h1>Disponibilite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ disponibilite.id }}</td>
            </tr>
            <tr>
                <th>Agent_id</th>
                <td>{{ disponibilite.agentId }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ disponibilite.debut ? disponibilite.debut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fin</th>
                <td>{{ disponibilite.fin ? disponibilite.fin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>All_day</th>
                <td>{{ disponibilite.allDay ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Background_color</th>
                <td>{{ disponibilite.backgroundColor }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_disponibilite_index') }}">back to list</a>

    <a href="{{ path('app_disponibilite_edit', {'id': disponibilite.id}) }}">edit</a>

    {{ include('disponibilite/_delete_form.html.twig') }}
{% endblock %}
